{
  "functions": {
    "app/api/transcribe/async/route.ts": {
      "maxDuration": 10
    },
    "app/api/transcribe/status/[jobId]/route.ts": {
      "maxDuration": 10
    },
    "app/api/upload/route.ts": {
      "maxDuration": 30
    },
    "app/api/cron/process-jobs/route.ts": {
      "maxDuration": 300
    },
    "app/api/cron/cleanup/route.ts": {
      "maxDuration": 60
    },
    "app/api/**/*": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron/process-jobs",
      "schedule": "*/1 * * * *"
    },
    {
      "path": "/api/cron/cleanup",
      "schedule": "0 3 * * *"
    }
  ]
}